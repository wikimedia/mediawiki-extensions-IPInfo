{
	"name": "IPInfo",
	"url": "https://www.mediawiki.org/wiki/Extension:IPInfo",
	"license-name": "MIT",
	"version": "0.0.0",
	"namemsg": "ipinfo-extensionname",
	"descriptionmsg": "ipinfo-desc",
	"manifest_version": 2,
	"requires": {
		"MediaWiki": ">= 1.36.0"
	},
	"MessagesDirs": {
		"IPInfo": [ "i18n" ]
	},
	"type": "other",
	"AutoloadNamespaces": {
		"MediaWiki\\IPInfo\\": "src/"
	},
	"TestAutoloadNamespaces": {
		"MediaWiki\\IPInfo\\Test\\Unit\\": "tests/phpunit/unit"
	},
	"ServiceWiringFiles": [
		"./ServiceWiring.php"
	],
	"AvailableRights": [
		"ipinfo"
	],
	"RestRoutes": [
		{
			"path": "/ipinfo/v0/revision/{id}",
			"method": "GET",
			"services": [
				"IPInfoInfoManager",
				"RevisionLookup",
				"PermissionManager",
				"UserFactory"
			],
			"factory": "MediaWiki\\IPInfo\\RestHandler\\RevisionHandler::factory"
		}
	]
}
