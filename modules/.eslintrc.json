{
	"root": true,
	"extends": [
		"wikimedia/client-es5",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"rules": {
		"no-jquery/no-global-selector": "off",
		"no-implicit-globals": "off",
		"no-jquery/no-extend": "warn"
	},
	"settings": {
		"polyfills": [
			"URL",
			"URLSearchParams"
		]
	}
}
